{
  "name": "iet-ou/ldap-ou",
  "description": "Lightweight Directory Access Protocol, for The OU. | Â© 2019 The Open University (IET).",
  "license": "proprietary",
  "time": "2019-02-05",
  "support": {
    "source": "https://gitlab.com/nfreear/ldap-ou.git"
  },
  "autoload": {
    "psr-4": {
      "IET_OU\\LdapOu\\": "src/"
    }
  },
  "require": {
    "zendframework/zend-ldap": "^2.10",
    "vlucas/phpdotenv": "^3.3"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "^1.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "scripts": {
    "test": [
      "@ldap",
      "parallel-lint src",
      "# php src/ldapou.php"
    ],
    "phpcs": "phpcs --standard=PSR2 src",
    "#": "composer ldap -v -- --oucu=xyzz123",
    "ldap": "IET_OU\\LdapOu\\LdapOu::test"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
